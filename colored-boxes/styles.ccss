body {
  background-color: #f4f4f4;
}

.color-grid {
  display: flex;
  gap: 15px;
  padding: 40px;
}

.color-box {
  width: 100px;
  height: 100px;
}

.color1 {
  background-color: #ff5733;
}

.color2 {
  background-color: rgb(52, 152, 219);
}

.color3 {
  background-color: green;
}

.color4 {
  background-color: hsl(280, 60%, 50%);
}

.color5 {
  background-color: #f1c40f;
}
